#!/usr/bin/env python
import click
from sorsched import db

@click.group()
def main():
    pass

@main.command(name='create-only')
def create_only():
    db.create_all()

@main.command(name='drop-and-create')
def drop_and_create():
    db.drop_all()
    db.create_all()


if __name__ == "__main__":
    main()

